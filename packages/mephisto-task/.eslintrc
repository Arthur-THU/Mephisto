{
  "extends": ["eslint:recommended", "plugin:react/recommended"],
  "plugins": ["import", "react", "react-hooks"],
  "parserOptions": {
    "ecmaVersion": 2018,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "settings": {
    "react": {
      "version": "detect"
    }
  },
  "rules": {
    "no-unused-vars": "warn",
    "no-console": "off"
  }
}
